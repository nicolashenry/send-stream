    "memfs": "~3.2.0",
    "mocha": "~8.0.1",
    "mongodb": "~3.5.9",
    "nyc": "~15.1.0",
    "on-finished": "~2.3.0",
    "rimraf": "~3.0.2",
    "should": "~13.2.3",
    "supertest": "~4.0.2",
    "ts-node": "~8.10.1",
    "typescript": "~3.9.5"
  },
  "license": "MIT",
  "dependencies": {
    "content-disposition": "^0.5.3",
    "mime": "^2.4.5",
    "range-parser": "^1.2.1",
