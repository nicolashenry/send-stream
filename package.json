{
  "name": "send-stream",
  "description": "Streaming file server with Range and conditional-GET support from file system or any streaming sources.",
  "repository": "github:nicolashenry/send-stream",
  "version": "0.3.0",
  "keywords": [
    "static",
    "file",
    "server",
    "send",
    "stream"
  ],
  "devDependencies": {
    "@types/content-disposition": "~0.5.3",
    "@types/express": "~4.17.6",
    "@types/koa": "~2.11.3",
    "@types/mime": "~2.0.1",
    "@types/mocha": "~7.0.2",
    "@types/mongodb": "~3.5.16",
    "@types/node": "~12.12.39",
    "@types/range-parser": "~1.2.3",
    "@types/supertest": "~2.0.9",
    "@typescript-eslint/eslint-plugin": "~2.33.0",
    "@typescript-eslint/parser": "~2.33.0",
    "coveralls": "~3.1.0",
    "cross-env": "~7.0.2",
    "eslint": "~7.0.0",
    "eslint-import-resolver-typescript": "~2.0.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-jsdoc": "~25.4.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-unicorn": "~20.0.0",
    "express": "~4.17.1",
    "fastify": "~2.14.1",
    "koa": "~2.11.0",
    "memfs": "~3.1.2",
    "mocha": "~7.1.2",
    "mongodb": "~3.5.7",
    "nyc": "~15.0.1",
    "on-finished": "~2.3.0",
    "rimraf": "~3.0.2",
    "should": "~13.2.3",
    "supertest": "~4.0.2",
    "ts-node": "~8.10.1",
    "typescript": "~3.9.2"
  },
  "license": "MIT",
  "dependencies": {
    "content-disposition": "^0.5.3",
    "mime": "^2.4.5",
    "range-parser": "^1.2.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "clean-build": "npm run clean && npm run build",
    "watch": "tsc -w",
    "prepare": "npm run clean-build",
    "prepublishOnly": "npm run test",
    "lint": "eslint . --ext .ts --max-warnings 0 --report-unused-disable-directives",
    "mocha": "nyc mocha test/*.spec.ts",
    "test": "npm run build && npm run lint && npm run mocha"
  },
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "files": [
    "dist/lib/*.js",
    "dist/lib/*.js.map",
    "dist/lib/*.d.ts"
  ],
  "nyc": {
    "include": [
      "index.ts",
      "lib/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcovonly",
      "text"
    ],
    "cache": false,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "engines": {
    "node": ">= 12.13.0"
  }
}
