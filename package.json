    "@types/koa": "~2.11.3",
    "@types/mime": "~2.0.2",
    "@types/mocha": "~7.0.2",
    "@types/mongodb": "~3.5.23",
    "@types/node": "~14.0.12",
    "@types/range-parser": "~1.2.3",
    "@types/supertest": "~2.0.9",
    "@typescript-eslint/eslint-plugin": "~3.3.0",
    "@typescript-eslint/parser": "~3.3.0",
    "coveralls": "~3.1.0",
    "cross-env": "~7.0.2",
    "eslint": "~7.4.0",
    "eslint-import-resolver-typescript": "~2.0.0",
